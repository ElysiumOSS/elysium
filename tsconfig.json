{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	},
	"include": ["src/**/*.ts", "src/**/*.astro", ".astro/types.d.ts"],
	"exclude": ["dist", "node_modules"]
}
